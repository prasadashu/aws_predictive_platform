{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "TrustRelationshipPolicy",
            "Effect": "Allow",
            "Principal": {
                "Service": "lambda.amazonaws.com"
            },
            "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents",
                "sts:AssumeRole"
            ],
            "Resource": "*"
        }
    ]
}